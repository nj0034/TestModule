{
    "meta": {
        "site_name": "문과대학",
        "site_url": "https://liberalarts.skku.edu/liberal/community/under_notice.do",
        "site_type": "html",
        "type": "공지사항",
        "ES_index": "skku-notice",
        "version": "1"
    },

    "parser": {
        "method": "url_based",
        "version": "1",
        "period": "15m",
        "stop": "size",
        "interval": "5-10"
    },

    "rule": {
        "page_url": "https://liberalarts.skku.edu/liberal/community/under_notice.do?mode=list&&articleLimit=10&article.offset={page}0",
        "start_page": 0,
        "page_increase": 1,
        "properties": {
            "title_url": {
                "type": "url",
                "xpath": "//*[@id=\"jwxe_main_content\"]/div/div/div/ul/li[*]/dl/dt/a",
                "position": "",
                "param_regex": "",
                "base_url": ""
            },
            "title": {
                "type": "text",
                "xpath": "//*[@id=\"jwxe_main_content\"]/div/div/div/div/div/div[1]/h4",
                "position": "in"
            },
            "created_datetime": {
                "type": "date",
                "xpath": "//*[@id=\"jwxe_main_content\"]/div/div/div/div/div/div[1]/ul/li[2]",
                "format": "%Y-%m-%d",
                "position": "in"
            },
            "content": {
                "type": "content",
                "xpath": "//*[@id=\"jwxe_main_content\"]/div/div/div/div/div/div[2]",
                "position": "in"
            },
            "img": {
                "type": "image",
                "xpath": "//*[@id=\"jwxe_main_content\"]/div/div/div/div/div/div[2]",
                "regex": "<img.*?src=\"(.*?)\"",
                "position": "in"
            },
            "attach": {
                "type": "file",
                "xpath": "//*[@id=\"jwxe_main_content\"]/div/div/div/div/div/ul/li[*]/a",
                "position": "in",
                "base_url": "",
                "param_regex": "",
                "name_regex": "<a.*>\\s*(.*)\\s*<\/a>",
                "HTML_xpath": "//*[@id=\"jwxe_main_content\"]/div/div/div/div/div/ul"
            }
        },
        "post_processing": {

        }
    }
}