{
  "meta": {
    "site_name": "명륜학사",
    "site_url": "https://dorm.skku.edu/skku_seoul/notice/notice_all.jsp?mode=list&search%3Asearch_val%3Asearch_or=&search%3Asearch_key%3Asearch_or=article_title&search%3Asearch_category%3Acategory=104&search%3Asearch_key2%3Asearch_or=article_text&board_no=78&pager.offset=0",
    "site_type": "html",
    "type": "공지사항",
    "ES_index": "skku-notice",
    "version": "1"
  },
  "parser": {
    "method": "url_based",
    "version": "1",
    "period": "15m",
    "stop": "size",
    "interval": "5-10"
  },
  "rule": {
    "page_url": "https://dorm.skku.edu/skku_seoul/notice/notice_all.jsp?mode=list&search%3Asearch_val%3Asearch_or=&search%3Asearch_key%3Asearch_or=article_title&search%3Asearch_category%3Acategory=104&search%3Asearch_key2%3Asearch_or=article_text&board_no=78&pager.offset={page}0",
    "start_page": 0,
    "page_increase": 1,
    "properties": {
      "title_url": {
        "type": "url",
        "xpath": "//*[@id=\"item_body\"]/div[3]/div/div[2]/div[1]/div[2]/div[2]/div/div[2]/table/tbody/tr[*]/td[3]/a",
        "position": "",
        "param_regex": "",
        "base_url": ""
      },
      "title": {
        "type": "text",
        "xpath": "//*[@id=\"item_body\"]/div[3]/div/div[2]/div[1]/div[2]/div[2]/div/div[1]/table/tbody/tr[1]/td",
        "position": "in"
      },
      "created_datetime": {
        "type": "date",
        "xpath": "//*[@id=\"item_body\"]/div[3]/div/div[2]/div[1]/div[2]/div[2]/div/div[1]/table/tbody/tr[2]/td/span[4]",
        "format": "%Y-%m-%d",
        "position": "in"
      },
      "content": {
        "type": "content",
        "xpath": "//*[@id=\"article_text\"]",
        "position": "in"
      },
      "img": {
        "type": "image",
        "xpath": "//*[@id=\"article_text\"]/p[*]/img",
        "position": "in"
      },
      "attach": {
        "type": "file",
        "xpath": "//*[@id=\"item_body\"]/div[3]/div/div[2]/div[1]/div[2]/div[2]/div/div[1]/table/tbody/tr[3]/td/div[*]/a",
        "position": "in",
        "base_url": "",
        "param_regex": "",
        "name_regex": "title=\"(.*) 다운로드\"",
        "HTML_xpath": "//*[@id=\"item_body\"]/div[3]/div/div[2]/div[1]/div[2]/div[2]/div/div[1]/table/tbody/tr[3]/td"
      }
    },
    "post_processing": {
    }
  }
}